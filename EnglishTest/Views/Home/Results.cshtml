@model Results

<markdown>

    ##### **Your Results** #####

</markdown>

<text>Correct Answers:</text><br>
@foreach (var typeTask in Model.CorrectTasks.Keys)
{
    <text>@typeTask: @Model.CorrectTasks[typeTask].ToString()</text><br>
}
<br>
<text>Incorrect Answers:</text><br>
@foreach (var typeTask in Model.IncorrectTasks.Keys)
{
    <text>@typeTask: @Model.IncorrectTasks[typeTask].ToString()</text><br>
}
<br>
<text>Max count: @Model.MaxCount</text><br>
<text>Your count: @Model.ResultCount</text><br>
<text>Result: @Model.Procent%</text><br>